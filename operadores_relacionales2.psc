	Definir valor1 Como Entero;
	Definir valor2 Como Entero;
	
	valor1 = 100;
	valor2 = 200;
	
	//distinto a 
	Escribir "Es distinto?", valor1 <> valor2
	
	//mayor o igual
	Escribir  valor1 >= valor2
	
	//menor o igual
